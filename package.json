{
  "name": "@zabaca/codegraph",
  "version": "0.1.3",
  "description": "TypeScript dependency graph analyzer for visualizing code relationships and analyzing impact of changes",
  "type": "module",
  "bin": {
    "codegraph": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun build src/cli.ts --outdir dist --target bun --packages external",
    "prepublishOnly": "bun run build",
    "dev": "bun --watch src/cli.ts",
    "test": "bun test",
    "codegraph": "bun run src/cli.ts"
  },
  "keywords": [
    "typescript",
    "dependency-graph",
    "code-analysis",
    "ast",
    "impact-analysis",
    "cli",
    "developer-tools",
    "static-analysis"
  ],
  "author": "zabaca",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zabaca/codegraph.git"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "nest-commander": "^3.20.1",
    "reflect-metadata": "^0.2.2",
    "ts-morph": "^27.0.2"
  }
}
